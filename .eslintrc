{
  // "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "$": true
  },
  "settings": {
    "react": {
      "version": "^17.0.1"
    }
  },
  "rules": {
    "semi": ["warn", "never"]
  }
}
